{
	"extends": "eslint:recommended",
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
	"rules": {
		// "arrow-body-style" : ["warn", "always"],
		"arrow-body-style" : 0,
		"arrow-parens" : ["warn", "always"],
		"no-undef" : 0,
		"no-unused-vars" : 0,
		"no-alert": 0,
		"no-console" : 0
	}
}